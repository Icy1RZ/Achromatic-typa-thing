	function spacebendingOMGOMGOMGOMMGMOMGMOMGMOGMOMGOMGOMGOM()

	Attack = true
	NeutralAnims = false
	local orig=WalkSpeed
	WalkSpeed=0
	legAnims=false
	

	for i = 0, 5, 0.1 do
		swait(1.5)
		local Alpha = .1
		Chat"You Shall fall in the abyss!"
										    				Effect({
Color = Color3.fromRGB(255, 255, 255),
Material = Enum.Material.Neon,
Mesh = "Slash1",
CFrame = Root.CFrame* CF.N(0,-3,0) * CF.A(0,M.R(math.random(-180,180)),10),
EndPos = Root.CFrame* CF.N(0,-3,0) * CF.A(0,M.R(math.random(-180,180)),-10),
Size = Vector3.new(1.9, 0.01, 1.9),
