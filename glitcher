WingAnims.NebCALAMITASsmol=function()
    LWP1W.C0 = LWP1W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(60+-55000*M.C(WingSine/20000)),M.R(0+5*M.C(WingSine/232))),.2)
    LWP2W.C0 = LWP2W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(120+-55000*M.C(WingSine/20000)),M.R(0+5*M.C(WingSine/232))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(180+-55000*M.C(WingSine/20000)),M.R(0+5*M.C(WingSine/232))),.2)
	
	RWP1W.C0 = RWP1W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(-60+-55000*M.C(WingSine/20000)),M.R(0+5*M.C(WingSine/232))),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(-120+-55000*M.C(WingSine/20000)),M.R(0+5*M.C(WingSine/232))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(1.5,-3,-4)*CF.A(5,M.R(-1+-55000*M.C(WingSine/20000)),M.R((0+5*M.C(WingSine/232)))),.2)
	RWP4W.C0 = RWP4W.C0:lerp(CF.N(0*M.C(WingSine/62),-1.64+0*M.S(WingSine/42),-3)*CF.A(-1.5,0,M.R(-0-0*M.C(WingSine/402))),.2)
