			end
			for i = 0, 60 do
				Txt.TextColor3=(Mode=='Troubadour' and Color3.fromHSV(hue/360,1,math.clamp(music.PlaybackLoudness/475,0,1)) or PrimaryColor)
				swait()
			end
			for i = 0, 1, .025 do
				Txt.TextTransparency=i
				swait()
			end
			BBG:destroy()
		end)()
		--else
		--	Chat2(text)
		--end
	end

	function Chat(text)
		Choot(text)
	end
	
	function Chat8(text)
		Choot8(text)
	end
	
	function Chat7(text)
		Choot7(text)
	end
	
		function Chat8(text)
		Choot8(text)
	end
	
    function Chat2(text)
		Choot2(text)
    end
    
