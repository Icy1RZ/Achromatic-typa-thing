	mult=mult or 1
	LWP1W.C0 = LWP1W.C0:lerp(CF.N(-0,1+0*M.C(WingSine/16),-3-0*M.C(WingSine/16))*CF.A(M.R(-25-0*M.C(WingSine/32)),0,M.R(0+4000*M.C(WingSine/500))),.2)
