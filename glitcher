WingAnims.blaze2=function(mult)
	mult=mult or 1
	LWP1W.C0 = LWP1W.C0:lerp(CF.N(3,2,1-4*M.C(WingSine/500))*CF.A(M.R(0+6000*M.C(WingSine/500)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	LWP2W.C0 = LWP2W.C0:lerp(CF.N(3,2,1-4*M.C(WingSine/500))*CF.A(M.R(-120+6000*M.C(WingSine/500)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(3,2,1-4*M.C(WingSine/500))*CF.A(M.R(-240+6000*M.C(WingSine/500)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	
	RWP1W.C0 = RWP1W.C0:lerp(CF.N(-3,2,10+5*M.C(WingSine/250))*CF.A(M.R(0+6000*M.C(WingSine/250)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(-3,2,10+5*M.C(WingSine/250))*CF.A(M.R(120+6000*M.C(WingSine/250)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(-3,2,10+5*M.C(WingSine/250))*CF.A(M.R(240+6000*M.C(WingSine/250)),0,M.R(0+6000*M.C(WingSine/500))),.2)
	RWP4W.C0 = RWP4W.C0:lerp(CF.N(0,-2,0)*CF.A(0,M.R(0*M.C(WingSine/1000)),M.R(0-WingSine))*CF.A(20.4,M.R(90),0)*CF.N(-0,3,0),.2)
	RWP5W.C0 = RWP5W.C0:lerp(CF.N(0*M.C(WingSine/62),0-0*M.S(WingSine/42),-2)*CF.A(0,0,M.R(-0+950*M.C(WingSine/200))),.2)	
