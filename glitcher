	end
	wait(1)
			for i = 0, 5, 0.1 do
		swait(0.1)
		local Alpha = .1
					RJ.C0 = RJ.C0:lerp(RJC0*CF.N(0,15+0*M.S(Sine/7),0)*CF.A(M.R(90+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32))),Alpha)
