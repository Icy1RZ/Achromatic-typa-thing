	{Name="POWER",Walkspeed=18,moveVal=8,Font=Enum.Font.Antique,StrokeColor=C3.RGB(226,155,64);Music=6904515615,LeftWing={0,BrickColor.new'Really black'.Color,Enum.Material.Neon};RightWing={0,BrickColor.new'Really black'.Color,Enum.Material.Neon};WingAnim='besmirch'};

