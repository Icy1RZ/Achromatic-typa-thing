					EndSize = Vector3.new(150, 50, 150),
					Transparency = NumberRange.new(0,1),
					Lifetime = 1,
				})
						Effect{
					Lifetime=.2;
					CFrame=Torso.CFrame*CF.N(0,0,0)*CF.A(M.RRNG(-360,360),M.RRNG(0,360),M.RRNG(-360,350));
					Mesh={Skull};
