					Transparency={0,1};
					Material=Enum.Material.Neon;
					Size=Vector3.new(10,10,10);
					EndSize=Vector3.new(1,1,1);
				}
				swait(4)
		end
	end)()


	WalkSpeed=orig
	legAnims=true
	Attack=false
	NeutralAnims=true
end
--epic attacks attacks end
UIS.InputBegan:connect(function(io,gpe)
	if(gpe or Attack or data.User~=data.Local)then return end
	--MODES
	if(io.KeyCode == Enum.KeyCode.One and Mode~='Redemption')then
		changeMode'Redemption'
	game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		 Chat"redeem yourself"
		 
	elseif(io.KeyCode == Enum.KeyCode.Two and Mode~='Curse')then 
		changeMode'Curse'
	game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		Chat"everyting.."
		wait(1)
		Chat"everyting is gone.."
		wait(1.1)
		Chat"Please stop.."
		
	elseif(io.KeyCode == Enum.KeyCode.Three and Mode~='darkened?')then 
		changeMode'darkened?'
		game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		 Chat"this ends today"
		
	elseif(io.KeyCode == Enum.KeyCode.Four and Mode~='Sacrifice')then 
		changeMode'Sacrifice'
		game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		 Chat"i hate this life"
		 
	elseif(io.KeyCode == Enum.KeyCode.Five and Mode~='Regret')then 
		changeMode'Regret'
		game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		 Chat"perhaps"
		 
    elseif(io.KeyCode == Enum.KeyCode.Six and Mode~='ANXIETY')then 
		changeMode'ANXIETY'
		game.lighting.ClockTime = 14 game.Lighting.FogEnd = 999999999999
		 Chat"MY WHOLE LIFE LIVING IN ANXIETY"
		 
