	LWP2W.C0 = LWP2W.C0:lerp(CF.N(.1*M.C(WingSine/32),0+0*M.S(WingSine/32),-5)*CF.A(0.2,0,M.R(0+0*M.C(WingSine/32)),M.R(0+0*M.C(WingSine/32))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(.1*M.C(WingSine/32),-1+0*M.S(WingSine/32),-5)*CF.A(0.2,0,M.R(0+180*M.C(WingSine/32)),M.R(0+0*M.C(WingSine/32))),.2)

	RWP1W.C0 = RWP1W.C0:lerp(CF.N(.1*M.C(WingSine/32),0+0*M.S(WingSine/32),-5)*CF.A(0.2,0,M.R(-90+0*M.C(WingSine/32)),M.R(0+0*M.C(WingSine/32))),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(.1*M.C(WingSine/32),0+0*M.S(WingSine/32),-5)*CF.A(0.2,0,M.R(180+0*M.C(WingSine/32)),M.R(0+0*M.C(WingSine/32))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(.1*M.C(WingSine/32),1+0*M.S(WingSine/32),-5)*CF.A(0.2,0,M.R(0+180*M.C(WingSine/32)),M.R(0+0*M.C(WingSine/32))),.2)
