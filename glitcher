	if v.Name == "MeshPartAccessory" and v.Handle.Size == Vector3.new(4, 4, 1) then
		v.Name = "MPASword" .. index
		index = index + 1
	elseif v.Name == "MeshPartAccessory" and v.Handle.Size == Vector3.new(3,3,3) then
		v.Name = "MPAAura"
	end
end

