	LWP2W.C0 = LWP2W.C0:lerp(CF.N(.1*M.C(WingSine/32),1.5+.1*M.C(WingSine/32),-1)*CF.A(0,0,M.R(80+50*M.C(WingSine/32))),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(.1*M.C(WingSine/32),1.5-.1*M.S(WingSine/32),-1)*CF.A(0,0,M.R(100+50*M.C(WingSine/32))),.2)

	RWP1W.C0 = RWP1W.C0:lerp(CF.N(-.1*M.C(WingSine/32),1.5-.1*M.C(WingSine/32),-1)*CF.A(0.5,0,M.R(-60-50*M.C(WingSine/42))),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(-.1*M.C(WingSine/32),1.5+.1*M.S(WingSine/32),-1)*CF.A(0.5,0,M.R(-80-50*M.C(WingSine/42))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(-.1*M.C(WingSine/32),1.5+.1*M.S(WingSine/32),-1)*CF.A(0.5,0,M.R(-100-50*M.C(WingSine/42))),.2)
	RWP4W.C0 = RWP4W.C0:lerp(CF.N(0*M.C(WingSine/62),-220.64+0*M.S(WingSine/42),-50)*CF.A(0,0,M.R(-0-950*M.C(WingSine/402))),.2)
	RWP5W.C0 = RWP5W.C0:lerp(CF.N(0*M.C(WingSine/62),0-0*M.S(WingSine/42),-2)*CF.A(0,0,M.R(-0+950*M.C(WingSine/402))),.2)	
end

WingAnims.Aprins=function()
	LWP1W.C0 = LWP1W.C0:lerp(CF.N(0,1.5,-1)*CF.A(0,M.R(0+2.5*M.C(WingSine/36)),M.R(60+5000*M.C(WingSine/400))),.2)
