})
					RJ.C0 = RJ.C0:lerp(RJC0*CF.N(0,-1.5+0*M.S(Sine/7),0)*CF.A(M.R(-35+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32))),Alpha)
									if(M.RNG(1,45)==1)then
					NK.C0 = NK.C0:lerp(NKC0*CF.A(M.RRNG(-360,25),M.RRNG(-50,25),M.RRNG(-25,25)),.8)
				else
					NK.C0 = NK.C0:lerp(NKC0*CF.N(0,0+.1*M.S(Sine/2),0)*CF.A(M.R(0),M.R(0),M.R(0-1*M.S(Sine/36))),Alpha)
				end
			    	LS.C0 = LS.C0:lerp(LSC0*CF.N(.5,-.1+.15*M.S(Sine/32),-.8)*CF.A(M.R(13),M.R(-12),M.R(104-2*M.S(Sine/36))),Alpha)
