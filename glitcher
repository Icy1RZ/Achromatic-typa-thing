				SoundPart(1368598393,.8,4,false,true,true,Root.CFrame*CF.N(0,0,-4-i*4))
				AOEDamage(Root.CFrame*CF.N(0,0,-4-i*4).p,5,{
					DamageColor=PrimaryColor;
					MinimumDamage=25;
					MaximumDamage=45;
				})

				Effect({
					Color = Color3.fromRGB(255,0,0),
					Material = Enum.Material.Neon,
					Mesh = "Blast",
					CFrame = Root.CFrame* CF.N(0,35,0) * CF.A(0,M.R(math.random(-180,180)),0),
					EndPos = Root.CFrame* CF.N(0,-3,0) * CF.A(0,M.R(math.random(-180,180)),0),
					Size = Vector3.new(0.01, 0.03, 0.01),
					EndSize = Vector3.new(150, 50, 150),
					Transparency = NumberRange.new(0,1),
					Lifetime = 1,
				})
				swait(4)
		end
	end)()


	WalkSpeed=orig
	legAnims=true
	Attack=false
	NeutralAnims=true
end
	function bruh()

	Attack = true
	NeutralAnims = false
	local orig=WalkSpeed
	WalkSpeed=0
	legAnims=false
	for i = 0, 5, 0.1 do
		swait(1)
		local Alpha = .1
		Chat"i warned you!"
					Effect({
                    Color = BrickColor.new'Black'.Color;
                    Material = Enum.Material.Grass,
                    Mesh = "Block",
                    Transparency={0,1};
                    CFrame = Root.CFrame* CF.N(math.random(-360,360),-3,math.random(-180,180)) * CF.A(1.5,M.R(math.random(-360,180)),0),
                    EndPos = Root.CFrame* CF.N(0,2,0) * CF.A(0,M.R(math.random(-180,180)),0),
                    Size = Vector3.new(2, 2, 2),
                    EndSize = Vector3.new(2, 2, 2),
                    wdLifetime = 0.1,
                })
					RJ.C0 = RJ.C0:lerp(RJC0*CF.N(0,-1+0*M.S(Sine/7),0)*CF.A(M.R(-28+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32))),Alpha)
					NK.C0 = NK.C0:lerp(NKC0*CF.N(0,0+0*M.S(Sine/7),0)*CF.A(M.R(0+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32)),M.R(0+0*M.C(Sine/32))),Alpha)
